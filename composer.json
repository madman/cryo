{
    "name": "madman/cryo",
    "require": {
        "php": ">=5.4",
        "ext-mcrypt": "*",
        "ext-mongo": "*",
        "silex/silex": "1.2.x-dev",
        "silex/web-profiler": "~1.0",
        "symfony/yaml": "2.6.3",
        "symfony/twig-bridge": "2.6.3",
        "symfony/finder": "<2.6-dev",
        "symfony/form": "2.5.*@dev",
        "symfony/validator": "2.5.*@dev",
        "symfony/config": "2.6.3",
        "symfony/security": "2.6.3",
        "symfony/console": "2.6.3",
        "symfony/monolog-bridge": "2.6.3",
        "symfony/property-access": "2.6.3",
        "symfony/options-resolver": "2.6.3",
        "symfony/security-csrf": "2.6.3",
        "twig/twig": ">=1.8,<2.0-dev",
        "monolog/monolog": ">=1.0.0",
        "ebidtech/collection": "@stable",
        "guzzlehttp/guzzle": "~4.0",
    },
    "require-dev": {
